doctype html
html(itemscope, itemtype="http://schema.org/WebPage")
	head
		title(itemprop="name")= title

		meta(name="viewport", content="width=device-width, initial-scale=1.0")

		link(href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.4/css/bootstrap.min.css", rel="stylesheet")
		link(href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.3.0/css/font-awesome.min.css", rel="stylesheet")

		link(href="https://fonts.googleapis.com/css?family=Roboto+Mono:400,100,300,500,700,100italic,400italic,500italic,700italic|Roboto+Condensed:300italic,400italic,700italic,400,300,700|Roboto:400,100,300,500,700,900,100italic,300italic,400italic,500italic,700italic,900italic", rel='stylesheet')

		link(href='/css/style.css', rel='stylesheet')

		block headClose

	body
		nav#site-navbar.navbar.navbar-default(role='navigation')
			.navbar-header
				button.navbar-toggle(type='button', data-toggle='collapse', data-target='#navbar-collapse')
					span.sr-only Toggle navigation
					span.icon-bar
					span.icon-bar
					span.icon-bar
				a.navbar-brand(href='/')
					| GW2 Guilds

			#navbar-collapse.collapse.navbar-collapse
				if(searchBar)
					form.guildSearch.navbar-form.navbar-right(action="/", role="search")
						.form-group
							input.guildName.form-control(name="guildName", type="text", placeholder="Guild Name")
						button.btn.btn-default(type="submit") Lookup

			//- 	ul.nav.navbar-nav.navbar-right
			//- 		li#indicator.navbarIcon.text-muted.hidden
			//- 			i.fa.fa-2x.fa-spinner.fa-spin.hidden
			//- 		li.divider-vertical
			//- 		li#audioToggle.navbarIcon.text-muted(data-enabled="false")
			//- 			span.fa-stack
			//- 				i.fa.fa-stack-2x.fa-ban.text-error
			//- 				i.fa.fa-stack-1x.fa-volume-up
			//- 		li.divider-vertical
			//- 		if(langs)
			//- 			each lang, langIndex in langs
			//- 				-var liClass = [];
			//- 				if urlLang == lang.key
			//- 					-liClass.push('active')
			//- 				li(class=liClass): a(href=lang.href, title=lang.name)= lang.label






		#content
			block content



		hr
		footer
			div.container
				div.row
					div.col-md-12.text-center
						p
							| Please send comments and bugs to <a class="contact"></a>
						p
							| Source available at
							a(href="https://github.com/fooey/node-gw2guilds", target="_blank")
								|  https://github.com/fooey/node-gw2guilds

						p
							a.btn.btn-lg.btn-danger(href="https://squareup.com/market/the-ln/donate-to-gw-w-w-com")
								i.fa.fa-heart
								| &nbsp; Donate to gw2w2w.com


						p.muted
							| Â© 2013 ArenaNet, Inc. All rights reserved. NCsoft, the interlocking NC logo, ArenaNet, Guild Wars, Guild Wars Factions,
							| Guild Wars Nightfall, Guild Wars: Eye of the North, Guild Wars 2, and all associated logos and designs are trademarks or
							| registered trademarks of NCsoft Corporation. All other trademarks are the property of their respective owners.

						-var now = new Date();
						p.muted
							span Generated #{now}
							span <br>Render time #{now - renderStart} ms


		script(src="https://cdn.polyfill.io/v1/polyfill.min.js")
		script(src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.min.js")
		script(src="https://cdnjs.cloudflare.com/ajax/libs/lodash.js/3.9.3/lodash.min.js")

		if (process.env.NODE_ENV === 'development')
			script(src="/js/dist/app.js")
		else
			script(src="/js/dist/app.min.js")

		script.
